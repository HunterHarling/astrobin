msgstr "(TAB ya da 4 boşluk)"

#: templates/faq.html:123
msgid "preformatted text"
msgstr "biçimlendirilmiş metin"

#: templates/faq.html:124
msgid "quoted text"
msgstr "alıntılanan metin"

#: templates/faq.html:125
msgid "escape the formatting syntax"
msgstr "formatlı yazımdan kaçının"

